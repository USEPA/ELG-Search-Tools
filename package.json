{
  "name": "elg",
  "scripts": {
    "build": "node server/src/build.js",
    "build:dev": "npm run migrate && npm run seed && vue-cli-service build",
    "build:prod": "vue-cli-service build",
    "prod": "node server/src/server.js",
    "lint": "eslint . --ext .js,.vue",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "server": "nodemon --ignore 'client/*' --inspect server/src/server.js",
    "client": "vue-cli-service serve",
    "start": "set NODE_ENV=local && concurrently --kill-others -n server,client \"npm run server\" \"npm run client\"",
    "test": "vue-cli-service test:unit"
  },
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.0.13",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.4",
    "bulma-checkradio": "^1.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dompurify": "^2.0.7",
    "dotenv": "^7.0.0",
    "exceljs": "^3.6.0",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "fibers": "^4.0.2",
    "jsdom": "latest",
    "lodash": "^4.17.15",
    "log4js": "^6.1.0",
    "nodemon": "^1.18.9",
    "pg": "^7.9.0",
    "register-service-worker": "^1.6.2",
    "sequelize": "^5.7.0",
    "sequelize-cli": "^5.4.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@types/express": "^4.17.2",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-pwa": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.16.4",
    "request-promise": "^4.2.5",
    "sass": "^1.17.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.5.21"
  }
}
