{
  "name": "elg-search-tools",
  "version": "1.0.0",
  "description": "The ‘ELG Search Tools’ application is a publicly accessible EPA/OWM sponsored web app that allows end users to search, filter and download ELG data.",
  "author": "Eastern Research Group (ERG)",
  "scripts": {
    "build": "node server/src/build.js",
    "lint": "vue-cli-service lint ./client/**/*.js ./client/**/*.vue",
    "lint:fix": "vue-cli-service lint --fix",
    "build:dev": "cross-env NODE_ENV=local npm run migrate && cross-env NODE_ENV=local npm run seed && cross-env NODE_ENV=local vue-cli-service build",
    "build:prod": "vue-cli-service build",
    "client": "vue-cli-service serve",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "prod": "node server/src/server.js",
    "seed": "sequelize db:seed:all",
    "server": "nodemon --ignore client/* --ignore server/src/s3/glossary.json --inspect server/src/server.js",
    "start": "cross-env NODE_ENV=local concurrently --kill-others -n server,client \"npm run server\" \"npm run client\"",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "aws-sdk": "^2.875.0",
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "bootstrap-vue": "^2.21.2",
    "bulma": "^0.7.4",
    "bulma-checkradio": "^1.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.12.1",
    "cors": "^2.8.5",
    "dompurify": "^2.2.8",
    "dotenv": "^7.0.0",
    "exceljs": "^3.10.0",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "fibers": "^4.0.3",
    "glossary-panel": "github:Eastern-Research-Group/glossary",
    "helmet": "^4.6.0",
    "jsdom": "^16.5.3",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "nodemon": "^1.19.4",
    "pg": "^7.18.2",
    "register-service-worker": "^1.7.2",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "vue": "^2.6.12",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.5.1",
    "vue-select": "^3.11.2",
    "vuex": "^3.6.2",
    "vuex-pathify": "^1.4.5",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@types/express": "^4.17.11",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/test-utils": "1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "prettier": "^1.16.4",
    "request-promise": "^4.2.6",
    "sass": "^1.32.12",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "bugs": {
    "url": "https://trello.com/b/P0wzFrwE/elg-database-web-app"
  },
  "engines": {
    "node": "14.x"
  },
  "homepage": "https://github.com/Eastern-Research-Group/ELG-Search-Tools#readme",
  "keywords": [
    "ELG",
    "Search"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eastern-Research-Group/ELG-Search-Tools.git"
  }
}
